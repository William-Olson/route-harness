{
  "name": "route-harness",
  "version": "2.1.4",
  "description": "Simple route harness for Express.",
  "types": "./build/index.d.ts",
  "main": "./index.js",
  "exports": "./build/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run clean && tsc -p . && ./fix-typings.sh",
    "lint": "tslint -p tsconfig.json",
    "format": "prettier --write \"lib/**/*.ts\"",
    "clean": "rm -rf ./build || true",
    "prepublish": "npm run build"
  },
  "keywords": [
    "route-harness",
    "express",
    "harness",
    "wrapper",
    "routes"
  ],
  "author": "Will Olson",
  "homepage": "http://github.com/William-Olson/route-harness",
  "repository": {
    "type": "git",
    "url": "git://github.com/William-Olson/route-harness.git"
  },
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "prettier": "^2.6.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0"
  }
}
